               in lib.mkOptionDefault {
                 "${modifier}+Return" = "exec ${pkgs.foot}/bin/foot";
                 "${modifier}+Shift+q" = "kill";
                 "${modifier}+d" = "exec ${pkgs.dmenu}/bin/dmenu_path | ${pkgs.dmenu}/bin/dmenu | ${pkgs.findutils}/bin/xargs swaymsg exec --";
               }

