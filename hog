                   { exec = "backspace"; on = [ "<Backspace>" ]; }
                 ];
                 manager.keymap = [
                   { exec = "escape"; on = [ "<Esc>" ]; }
                   { exec = "quit"; on = [ "q" ]; }
                   { exec = "close"; on = [ "<C-q>" ]; }
                 ];
