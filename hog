           [4mType:[24m attribute set of attribute set of (INI atom (null, bool, int, float or string))

           [4mDefault:[24m { }

           [4mExample:[0m

               {
                 "*" = {
                   charset = "utf-8";
                   end_of_line = "lf";
                   trim_trailing_whitespace = true;
                   insert_final_newline = true;
                   max_line_width = 78;
                   indent_style = "space";
